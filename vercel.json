{
  "routes": [
    {
      "src": "/",
      "dest": "/Pages/index.html"
    },
    {
      "src": "/about",
      "dest": "/Pages/AboutUsPage.html"
    },
    {
      "src": "/classes",
      "dest": "/Pages/SubjectsPage.html"
    },
    {
      "src": "/contact",
      "dest": "/Pages/ContactPage.html"
    },
    {
      "src": "/all-subjects",
      "dest": "/Pages/GridViewSubjects.html"
    },
    {
      "src": "/faqs",
      "dest": "/Pages/FAQsPage.html"
    },
    {
      "src": "/testimonials",
      "dest": "/Pages/TestimonialsPage.html"
    },
    {
      "src": "/subjects/(.*)",
      "dest": "/Pages/generated/$1"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false,
  "public": true
}
