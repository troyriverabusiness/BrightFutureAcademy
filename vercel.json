{
  "rewrites": [
    {
      "source": "/",
      "destination": "/Pages/index.html"
    },
    {
      "source": "/about",
      "destination": "/Pages/AboutUsPage.html"
    },
    {
      "source": "/classes",
      "destination": "/Pages/SubjectsPage.html"
    },
    {
      "source": "/contact",
      "destination": "/Pages/ContactPage.html"
    },
    {
      "source": "/all-subjects",
      "destination": "/Pages/GridViewSubjects.html"
    },
    {
      "source": "/faqs",
      "destination": "/Pages/FAQsPage.html"
    },
    {
      "source": "/testimonials",
      "destination": "/Pages/TestimonialsPage.html"
    },
    {
      "source": "/subjects/:path*",
      "destination": "/Pages/generated/:path*"
    }
  ],
  "headers": [
    {
      "source": "/css/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/css"
        }
      ]
    },
    {
      "source": "/js/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    },
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "image/*"
        }
      ]
    },
    {
      "source": "/icons/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "image/*"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false,
  "public": true,
  "functions": {},
  "outputDirectory": "."
}
