{
  "rewrites": [
    {
      "source": "/",
      "destination": "/Pages/index.html"
    },
    {
      "source": "/about",
      "destination": "/Pages/AboutUsPage.html"
    },
    {
      "source": "/classes",
      "destination": "/Pages/SubjectsPage.html"
    },
    {
      "source": "/contact",
      "destination": "/Pages/ContactPage.html"
    },
    {
      "source": "/all-subjects",
      "destination": "/Pages/GridViewSubjects.html"
    },
    {
      "source": "/faqs",
      "destination": "/Pages/FAQsPage.html"
    },
    {
      "source": "/testimonials",
      "destination": "/Pages/TestimonialsPage.html"
    },
    {
      "source": "/subjects/:path*",
      "destination": "/Pages/generated/:path*"
    }
  ],
  "routes": [
    {
      "src": "/css/(.*)",
      "dest": "/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/js/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "src": "/icons/(.*)",
      "dest": "/icons/$1"
    }
  ],
  "functions": {},
  "cleanUrls": true,
  "trailingSlash": false
}
